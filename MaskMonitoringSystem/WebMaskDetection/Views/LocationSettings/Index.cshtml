@model IEnumerable<WebMaskDetection.Models.LocationSetting>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Location List</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Location)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.locationIndex">Edit</a> |
                <!-- <a asp-action="Details" asp-route-id="@item.locationIndex">Details</a> |-->
                <a asp-action="Delete" asp-route-id="@item.locationIndex">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
